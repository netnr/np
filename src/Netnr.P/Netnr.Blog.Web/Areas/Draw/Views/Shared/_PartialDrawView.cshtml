<script type="text/javascript" src="@mxBaseServer/examples/grapheditor/www/jscolor/jscolor.js"></script>
<script type="text/javascript" src="@mxBaseServer/examples/grapheditor/www/sanitizer/sanitizer.min.js"></script>
<script type="text/javascript" src="@mxBaseServer/mxClient.min.js"></script>
<script type="text/javascript" src="@mxBaseServer/examples/grapheditor/www/js/EditorUi.js"></script>
<script type="text/javascript" src="@mxBaseServer/examples/grapheditor/www/js/Editor.js"></script>
<script type="text/javascript" src="@mxBaseServer/examples/grapheditor/www/js/Sidebar.js"></script>
<script type="text/javascript" src="@mxBaseServer/examples/grapheditor/www/js/Graph.js"></script>
