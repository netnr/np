@{
    ViewData["Title"] = "draw";

    var mxBaseServer = "https://netnr.github.io/mxgraph/javascript";
}

@Html.Raw(Netnr.SharedApp.QuoteTo.Html("loading"))

<input type="hidden" id="mxBaseServer" value='@mxBaseServer' />

<input type="hidden" id="vid" value='@ViewData["vid"]' />

<link href="@mxBaseServer/examples/grapheditor/www/styles/grapheditor.css" rel="stylesheet" />

<style>
    /*重写菜单样式*/
    html td.mxPopupMenuItem {
        font-size: 12pt;
        padding: 3px 30px;
    }

    /*全屏*/
    .viewFull .geDiagramContainer {
        position: absolute !important;
        top: 0 !important;
        left: 0 !important;
        right: 0 !important;
        z-index: 1;
    }
</style>

<script src="/areas/draw/js/partial/mxg.js" asp-append-version="true"></script>

<script type="text/javascript" src="@mxBaseServer/examples/grapheditor/www/deflate/pako.min.js"></script>
