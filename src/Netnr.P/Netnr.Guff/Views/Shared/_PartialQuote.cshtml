@model string
@{
    var listQuote = Model.Split(',').ToList();
}
@foreach (var item in listQuote)
{
    switch (item)
    {
        case "global.css":
            <link href="/css/global.css" rel="stylesheet" asp-append-version="true" />
            break;

        case "global.js":
            <script src="/js/global.js" asp-append-version="true"></script>
            break;
    }
}