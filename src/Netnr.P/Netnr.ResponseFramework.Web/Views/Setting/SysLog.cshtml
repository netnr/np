@{
    ViewData["Title"] = "操作日志";
}

@await Component.InvokeAsync("Form", new InvokeFormVM()
{
    TableName = "SysLog"
})

@if (!GlobalTo.GetValue<bool>("logs:enable"))
{
    <script>
        setTimeout(function () {
            art('系统已关闭日志记录')
        }, 1000 * 2);
    </script>
}

<partial name="_BaseViewPartial" />